{
  "name": "vite-react",
  "version": "0.0.0",
  "description": "基于vite2的react脚手架",
  "license": "MIT",
  "scripts": {
    "dev": "vite --mode development",
    "build:beta": "vite build --mode beta",
    "build:release": "tsc && vite build --mode release",
    "serve": "vite preview",
    "eslint": "eslint --ext .tsx,.ts --fix ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --cache --fix",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --cache --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@types/qs": "^6.9.6",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "antd": "^4.16.6",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "dotenv": "^10.0.0",
    "eslint-config-alloy": "^4.1.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "4.3.8",
    "jsencrypt": "^3.2.0",
    "less": "^4.1.1",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^11.0.0",
    "qs": "^6.10.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-cache-route": "^1.11.1",
    "react-router-dom": "^5.2.0",
    "vite-plugin-imp": "^2.0.7"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.2",
    "vite": "^2.3.8"
  }
}
